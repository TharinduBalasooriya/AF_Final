.admin-body {
    font-family: "Lato", sans-serif;
    height: 100%;

    background-image: url('9999dec4ee56392db2ffe6f60624dff4');

}
.admin-title{
    text-align: center;
    font-family: 'Times New Roman', serif;
    color: whitesmoke;
     margin-top: 40px;
    margin-bottom: 100px;
    font-size: 80px;
}
p, span, a, ul, li, button {
    font-family: inherit;
    font-size: inherit;
    font-weight: inherit;
    line-height: inherit;
}
strong {
    font-weight: 600;
}
h1, h2, h3, h4, h5, h6 {
    font-family: 'Open Sans', "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", Arial, sans-serify;
    line-height: 1.5em;
    font-weight: 300;
}
strong {
    font-weight: 400;
}
.tile {
    width: 100%;
    display: inline-block;
    box-sizing: border-box;
    background: #fff;
    padding: 20px;
    margin-bottom: 30px;
}
.title {
    margin-top: 0px;
}
.purple, .blue, .red, .orange, .green {
    color: #fff;
}
.purple {
    background: #5133AB;
}
.red { background: #AC193D;}
.green {background: #00A600;}
.blue {   background: #2672EC;}
.orange {  background: #DC572E;}

